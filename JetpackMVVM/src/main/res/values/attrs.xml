<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ClearEditText">
        <attr name="cleanDrawable" format="reference"/>
    </declare-styleable>

    <declare-styleable name="PasswordEditText">
        <attr name="eyeVisibleDrawable" format="reference"/>
        <attr name="eyeInvisibleDrawable" format="reference"/>
    </declare-styleable>

    
  
    
    <declare-styleable name="ShapeImageView">
        <attr name="shape" format="enum">
            <enum name="rect" value="0"/>
            <enum name="circle" value="1"/>
        </attr>
        <attr name="cornerRadius" format="dimension"/>
    </declare-styleable>
    
    <declare-styleable name="StateSwitcher">
        
        <attr name="loading_layout" format="reference"/>
        <attr name="network_error_layout" format="reference"/>
        <attr name="empty_layout" format="reference"/>
    </declare-styleable>

</resources>